<div class="container">

<div class="container form">
  
<%= simple_form_for @car do |f| %>
  <div class="container-new-car">
  <h1 class="bold-text">Add car</h1>
</div>
  <%= f.input :model %>
   <%= f.input :price %>
    <%= f.input :fuel_type %>
     <%= f.input :consumption %>
      <%= f.input :category %>
       <%= f.input :seat_number %>
        <%= f.input :transmission %>
        <%= f.input :address %>
        <%= f.input :photo, as: :file %>

  <%= f.submit "Submit car", class: "btn btn-primary" %>
<% end %>
</div>

</div>

